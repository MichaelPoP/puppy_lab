<h1>NEW PUPPIES!</h1>
<% if @puppy.errors.any? %>
  <% @puppy.errors.full_messages.each do |error| %>
    <p class="text-danger">
      <%= error %>
    </p>
  <% end %>
<% end %>

<%= form_for @puppy, url: "/puppies" do |f|  %>
<p>
  <%= f.label :name  %>
  <%= f.text_field :name, autofocus: true %>
</p>
<p>
  <%= f.label :age  %>
  <%= f.text_field :age  %>
</p>
<p>
  <%= f.label :breed  %>
  <%= f.text_field :breed  %>
</p>
<p>
  <%= f.submit  %>
</p>

<% end  %>
